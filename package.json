    "zapier": "npm run build && zapier-platform",
    "lint": "eslint  './src/**/*.ts' --report-unused-disable-directives && echo 'Your .ts files look good.'",
    "lint-fix": "eslint './src/**/*.ts' --fix"
  },
  "engines": {
    "node": "^22"
  },
  "dependencies": {
    "zapier-platform-cli": "18.0.5",
    "zapier-platform-core": "18.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~8.48",
    "vitest": "~3.2",
    "mocha": "~11.7",
    "rimraf": "~6.1",
    "ts-node": "~10.9.2",
    "typescript": "~5.9.3"
  },
  "overrides": {
    "@types/node": "^22"
  },
  "private": true,
  "exports": "./dist/index.js",
  "type": "module"
